<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5dip" />
            <gradient
                android:startColor="@color/primary300"
                android:centerColor="@color/primary300"
                android:endColor="@color/primary300"
                />
        </shape>
    </item>

    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5dip" />
                <gradient
                    android:startColor="@color/chartGreen"
                    android:centerColor="@color/chartGreen"
                    android:endColor="@color/chartGreen"
                    />
            </shape>
        </clip>
    </item>

    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="5dip" />
                <gradient
                    android:startColor="@color/chartPink"
                    android:centerColor="@color/chartPink"
                    android:endColor="@color/chartPink"
                    />
            </shape>
        </clip>
    </item>





</layer-list>